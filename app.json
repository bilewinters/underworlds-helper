{
  "expo": {
    "name": "Underworlds",
    "description": "Underworlds Helper - helper for Underworlds gaming",
    "slug": "uwhelper",
    "privacy": "unlisted",
    "sdkVersion": "34.0.0",
    "platforms": ["ios", "android"],
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bg/icon.png",
    "splash": {
      "image": "./assets/images/bg/bg_teeth.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wordsspeaklouder.uwhelper",
      "buildNumber": "1",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.wordsspeaklouder.uwhelper",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bg/icon.png",
        "backgroundColor": "#883b0d"
      },
      "permissions": []
    }
  }
}
