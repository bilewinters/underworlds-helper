{
  "expo": {
    "name": "Underworlds",
    "description": "Underworlds Helper - helper for Underworlds gaming",
    "slug": "uwhelper",
    "privacy": "unlisted",
    "sdkVersion": "34.0.0",
    "platforms": ["ios", "android"],
    "version": "2.0.1",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/images/bg/bg_plain.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": { "fallbackToCacheTimeout": 0, "checkAutomatically": "ON_ERROR_RECOVERY" },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wordsspeaklouder.uwhelper",
      "buildNumber": "1",
      "icon": "./assets/images/bg/iconIos.png",
      "config": { "usesNonExemptEncryption": false }
    },
    "android": {
      "package": "com.wordsspeaklouder.uwhelper",
      "versionCode": 1,
      "icon": "./assets/images/bg/iconAndroid.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bg/iconAndroidFG.png",
        "backgroundColor": "#530308"
      },
      "permissions": []
    }
  }
}
